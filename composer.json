{
  "name": "batenburg/jwt-guard",
  "description": "Verify JWT for Laravel",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Patrick Batenburg",
      "email": "p.batenburg@live.nl"
    }
  ],
  "require": {
    "php": ">= 7.4.0",
    "laravel/framework": "^8.0",
    "batenburg/jwt-verifier": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Batenburg\\JWTGuard\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Batenburg\\JWTGuard\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
